{
  "name": "anova-react-github",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@react-cmpt/react-request-hook": "^3.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "grommet": "^2.17.2",
    "grommet-icons": "^4.5.0",
    "msw": "^0.28.2",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "postinstall": "husky install",
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.7",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0"
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "proxy": "https://api.github.com",
  "husky": {
    "hooks": {
      "pre-commit": "yarn run test:ci && yarn lint-staged"
    }
  }
}
